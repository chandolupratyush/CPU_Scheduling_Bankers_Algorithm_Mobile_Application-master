<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.cpuscheduling">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Scheduling" />
        <activity android:name=".Deadlock" />
        <activity android:name=".Sjf" />
        <activity android:name=".Srtf" />
        <activity android:name=".Lrtf" />
        <activity android:name=".Fcfs" />
        <activity android:name=".Roundrobin" />
        <activity android:name=".Pp" />
        <activity android:name=".pnp" />
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>